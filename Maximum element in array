/* C program to get maximum element of an array */

#include <stdio.h>

#define MAX 10

int main()
{
  int arr[MAX];
  int i,max_ele;
  for (i=0; i<MAX; i++)
  {
    scanf("%d", (arr+ i));
    }
    
    max_ele = *(arr+0);
    
    for( i= 1; i<MAX;i++)
    {
       if( *(arr+i) > max_ele)
           max_ele = *(arr+i);
    }
    
    return 0;
  }
      
  
