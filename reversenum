/* C program to reverse a number*/

#include <stdio.h>

int main()
{
   int n,reverse,t;
   printf("Enter a number\n");
   scanf("%d", &n);
   reverse =0;
   t = n;
   while(n!=0)
   {
      reverse = reverse*10;
      reverse = reverse + (n%10);
      n = n/10;
      }
   printf("Reverse of %d is %d \n",t,reverse);
   return 0;
   }
   
   
   
   
   
   
