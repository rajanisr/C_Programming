/* C program to get minimum element of an array */

#include <stdio.h>

#define MAX 10

int main()
{
  int arr[MAX];
  int i,min_ele;
  for (i=0; i<MAX; i++)
  {
    scanf("%d", (arr+ i));
    }
    
    min_ele = *(arr+0);
    
    for( i= 1; i<MAX;i++)
    {
       if( *(arr+i) < min_ele)
           min_ele = *(arr+i);
    }
    
    return 0;
  }
