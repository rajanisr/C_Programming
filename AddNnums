/* C program to add n numbers*/

#include <stdio.h>

int main()
{
   int n;
   float num,sum =0;
   int i;
   
   printf("Enter the number of numbers\n");
   scanf("%d",&n);
   printf("Enter the numbers\n");   
   for(i =1; i<=n ; i++)
    {
      scanf("%f", &num);
      sum+= num;
    }
      
   printf("The sum is %f\n", sum);
   return 0;
   }
